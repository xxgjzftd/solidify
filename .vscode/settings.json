{
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.patterns": {
    ".gitignore": ".gitattributes, .gitmodules, .gitmessage, .lfsconfig, .mailmap, .git-blame*",
    "tsconfig.json": "tsconfig.*.json, *.d.ts",
    "vite.config.*": "postcss.config.*, tailwind.config.*",
    "package.json": "package-lock.json, eslint.config.*, prettier.config.*",
    "Dockerfile": "*.Dockerfile, nginx.conf"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": { "source.fixAll.eslint": "explicit" },
  "cSpell.words": [
    "Aliyun",
    "Heroicons",
    "iconify",
    "mailihome",
    "openapi",
    "sketchup",
    "solidjs",
    "sortablejs",
    "unplugin"
  ],
  "files.associations": { "*.css": "tailwindcss" },
  "tailwindCSS.classFunctions": ["tw", "cva", "cx"],
  "tailwindCSS.classAttributes": ["class", "ui"],
  "editor.quickSuggestions": { "strings": "on" }
}
